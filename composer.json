{
    "name": "lishuji/laravel-comments",
    "description": "Comments for Laravel9.",
    "keywords": [
        "laravel9",
        "comments"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "lishuji"
        }
    ],
    "require": {
        "php": "^8.1",
        "erusev/parsedown": "^1.7",
        "illuminate/database": "^9.0",
        "illuminate/http": "^9.0",
        "illuminate/pagination": "^9.0",
        "illuminate/routing": "^9.0",
        "illuminate/queue": "^9.0",
        "illuminate/support": "^9.0",
        "spatie/laravel-honeypot": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Lishuji\\Comments\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lishuji\\Comments\\ServiceProvider"
            ]
        }
    }
}
